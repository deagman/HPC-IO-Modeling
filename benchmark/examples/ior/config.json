{
    "name" : "myexperiment",
    "result_folder" : "/an/absolute/path",
    "mode" : "batch",
    "batchsize" : 2,

    "slurm_shell": {
      "file_path": "./slurm_shell",
      "parameters": {
        "transfersize, blocksize": ["512k, 2m", "1m, 8m"],
        "segment": ["1"]
      }
    },
    "input_file_1": {
      "file_path": "romio_hints",
      "parameters": {
        "__striping_factor": ["1","2"],
        "__striping_unit": ["1048576","2097152"]
      }
    },
    "output_file_1": {
      "file_type": "out",
      "lines" :[
          "write         $write_bw",
          "read      $read_bw"
      ]
    },
    "output_file_2": {
      "file_type" : "darshan",
      "lines" : []
    }
  }